{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
		"group": "legacy",
		"src": "legacy",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
	  {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "active",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ],
        "src": "active"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ],
		"src": "legacy",
        "group": "legacy"
      },
	  {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "src": "active",
        "group": "active"
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/e2eprod-version/breadcrumb/toc.json",
      "extendBreadcrumb": true
    },
    "fileMetadata": {},
    "template": [],
    "dest": "e2eprod-version",
    "markdownEngineName": "markdig",
	"groups": {
      "legacy": {
        "dest": "legacy",
        "moniker_range": "< contososerver-2.0"
      }, 
      "active": {
        "dest": "active",
        "moniker_range": ">= contososerver-2.0 || >= contosoclient-1.0"
      }
    }	
  }
}